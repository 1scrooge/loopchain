{
  "log": {
    "logger": "iconrpcserver",
    "colorLog": true,
    "level": "debug",
    "filePath": "./log/develop/iconrpcserver.log",
    "outputType": "console|file",
    "rotate": {
      "type": "period|bytes",
      "period": "daily",
      "interval": 1,
      "backupCount": 10,
      "maxBytes": 50000000
    }
  },
  "gunicornWorkerCount": 1
}
